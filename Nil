--[[task.spawn(function()
	while task.wait()do
		for i,v in pairs(OURGARRAGE.PARTS:GetChildren()) do
			if v.Name == "Water Buoy" then
				if v:FindFirstChildWhichIsA("AlignPosition") and v:FindFirstChildWhichIsA("BodyGyro") then

				else
					local Force = Instance.new("AlignPosition",v)
					Force.MaxVelocity = math.huge
					Force.MaxForce = math.huge
					Force.Mode = Enum.PositionAlignmentMode.OneAttachment
					local Gyro = Instance.new("BodyGyro",v)
					Gyro.MaxTorque = Vector3.new(math.huge,math.huge,math.huge)
					local Attachment = Instance.new("Attachment",v)
					Force.Attachment0 = Attachment
					Force.ForceLimitMode = Enum.ForceLimitMode.Magnitude
					table.insert(CONFIG.BUOY.Forces,{v,Force,Gyro,Attachment})

					local HL = Instance.new("Highlight",v)
					HL.Adornee = v
					HL.OutlineTransparency = 0
					HL.FillTransparency = .5
				end
			end
		end
		for i,v in pairs(CONFIG.BUOY.Forces) do
			if v[2].Parent.Parent ~= OURGARRAGE.PARTS then
				v[2]:Destroy()
				v[3]:Destroy()
				table.remove(CONFIG.BUOY.Forces,i)
			end
			if CONFIG.BUOY.ACTIVE == true then
				if CONFIG.BUOY.CLICKENABLE == false then
					if CONFIG.BUOY.TARGET ~= nil then
						CONFIG.BUOY.POSITION = CONFIG.BUOY.TARGET.Position
					end
				end

				v[2].MaxForce = math.huge
				v[2].MaxVelocity = math.huge
				v[3].MaxTorque = Vector3.new(math.huge,math.huge,math.huge)
				v[2].Position = CONFIG.BUOY.POSITION
				v[2].RigidityEnabled = CONFIG.BUOY.ENFORCEDRIGIDITY
				v[2].Responsiveness = CONFIG.BUOY.SPEED
				v[3].D = CONFIG.BUOY.GRYOSTUFF.SMOOTHNESS
				v[3].P = CONFIG.BUOY.GRYOSTUFF.SPEED
				if CONFIG.BUOY.FORMATION == 1 then
					v[3].CFrame = CFrame.lookAt(v[1].Position,CONFIG.BUOY.POSITION)
				elseif CONFIG.BUOY.FORMATION ==2 then
					v[3].CFrame = CFrame.new()*CFrame.fromEulerAngles(math.rad(math.random(1,360)),math.rad(math.random(1,360)),math.rad(math.random(1,360)))
				end
			else
				v[2].RigidityEnabled =false
				v[2].MaxForce = 0
				v[2].MaxVelocity = 0
				v[3].MaxTorque = Vector3.new(0,0,0)
			end
		end
	end

end)]]
task.spawn(function()
	while task.wait() do
		pcall(function()
			while CONFIG.GOKUSTUFF.MASTEREDULTRAINSTINCT == true do
				local CHAR = SERVICES.Player.LocalPlayer.Character
				local HRP = CHAR:FindFirstChild("HumanoidRootPart")
				if HRP == nil then
					warn("YOU ARE DEAD.") --KILL YOURSELF
				end
				local GOKUPART = Instance.new('Part')
				GOKUPART.Position = HRP.Position
				GOKUPART.Size = Vector3.new(CONFIG.GOKUSTUFF.AREASIZE,CONFIG.GOKUSTUFF.AREASIZE,CONFIG.GOKUSTUFF.AREASIZE)
				local OLDHRPPOS = HRP.CFrame
				for i,v in pairs(SERVICES.Work:GetPartsInPart(GOKUPART)) do
					if (string.find(v.Name, "Bumper") or string.find(v.Name, "Spike")) and (not (v:IsDescendantOf(OURGARRAGE.PARTS))) then
						task.spawn(function()
							CHAR:PivotTo(OLDHRPPOS + (CONFIG.GOKUSTUFF.AREASIZE*(OLDHRPPOS.Position - v.Position).Unit))
							--local Force = Instance.new("BodyForce",HRP)
							--Force.Force = 25000*(HRP.Position - v.Position).Unit
						end)
						if CONFIG.GOKUSTUFF.GOKUNOISES == true then
							local Sound = Instance.new("Sound",SERVICES.Sound)
							Sound.SoundId = "rbxassetid://6820466812"
							Sound.Playing = true
							SERVICES.Deb:AddItem(Sound,2)
						end
					else
						GOKUPART.Anchored = true
						GOKUPART.CanCollide = false
						GOKUPART.Transparency = .5
						GOKUPART.Parent = workspace
						SERVICES.Deb:AddItem(GOKUPART,.1)

					end
				end
				task.wait(CONFIG.GOKUSTUFF.FRAMETIME)
			end
		end)
	end	
end)
local Mouse = SERVICES.Player.LocalPlayer:GetMouse()
Mouse.Button1Down:Connect(function()
	if CONFIG.BUOY.CLICKENABLE == true then
		local Temp = Mouse.Hit
		CONFIG.BUOY.POSITION =  Vector3.new(Temp.X,Temp.Y,Temp.Z)
	end
end)
function InpBegan(Inp,GP)
	if SERVICES.UInp:IsKeyDown(Enum.KeyCode.P)then
		OpenCheck()
	end
end
function ScanMyNiggers()
	for i,v in pairs(GuiNChildren["PlayerScrolling"]:GetChildren()) do
		if not (v:IsA("UIListLayout")) then
			v:Destroy()
		end
	end
	for i,v in pairs(SERVICES.Player:GetChildren()) do
		if not v:IsA("Player") then return end
		local SpecializedButton = script.PLAYERBTN:Clone()
		SpecializedButton.Text = v.Name
		SpecializedButton.LayoutOrder = i
		SpecializedButton.Activated:Connect(function()
			local OtherHRP = v.Character:FindFirstChild("HumanoidRootPart")
			repeat
				OtherHRP = v.Character:FindFirstChild("HumanoidRootPart")
				task.wait()
			until OtherHRP ~= nil
			CONFIG.BUOY.TARGET = OtherHRP
		end)
		SpecializedButton.Parent = GuiNChildren["PlayerScrolling"]
	end
end
ScanMyNiggers()
SERVICES.Player.ChildAdded:Connect(ScanMyNiggers)
SERVICES.Player.ChildRemoved:Connect(ScanMyNiggers)
SERVICES.UInp.InputBegan:Connect(InpBegan)
SERVICES.Run.Heartbeat:Connect(function()

	for i,v in pairs(SERVICES.Work:GetDescendants()) do
		if v:IsA("Sound") and (v~= CONFIG.SOUNDSTUFF.SOUNDREPLACER) then
			v.SoundGroup = CONFIG.SOUNDSTUFF.SOUNDMUTER
		end
	end
	if CONFIG.SOUNDSTUFF.MUTED == true then
		CONFIG.SOUNDSTUFF.SOUNDMUTER.Volume = 0
	else
		CONFIG.SOUNDSTUFF.SOUNDMUTER.Volume = 1
	end
	--SOUND STUFF END
	for i,v in pairs(OURGARRAGE.PARTS:GetChildren()) do
		if v.Name == "Water Buoy" then
			if v:FindFirstChildWhichIsA("AlignPosition") and v:FindFirstChildWhichIsA("BodyGyro") then

			else
				local Force = Instance.new("AlignPosition",v)
				Force.MaxVelocity = math.huge
				Force.MaxForce = math.huge
				Force.Mode = Enum.PositionAlignmentMode.OneAttachment
				local Gyro = Instance.new("BodyGyro",v)
				Gyro.MaxTorque = Vector3.new(math.huge,math.huge,math.huge)
				local Attachment = Instance.new("Attachment",v)
				Force.Attachment0 = Attachment
				Force.ForceLimitMode = Enum.ForceLimitMode.Magnitude
				table.insert(CONFIG.BUOY.Forces,{v,Force,Gyro,Attachment})

				local HL = Instance.new("Highlight",v)
				HL.Adornee = v
				HL.OutlineTransparency = 0
				HL.FillTransparency = .5
			end
		end
	end
	for i,v in pairs(CONFIG.BUOY.Forces) do
		local s,e = pcall(function()
			if (v[2].Parent.Parent ~= OURGARRAGE.PARTS) or (v[3].Parent.Parent ~= OURGARRAGE.PARTS) or (v[4].Parent.Parent ~= OURGARRAGE.PARTS) then
				v[2]:Destroy()
				v[3]:Destroy()
				v[4]:Destroy()
				table.remove(CONFIG.BUOY.Forces,i)
			end
		end)
		if e ~= nil then
			v[2]:Destroy()
			v[3]:Destroy()
			v[4]:Destroy()
		end
		if CONFIG.BUOY.ACTIVE == true then
			if CONFIG.BUOY.CLICKENABLE == false then
				if CONFIG.BUOY.TARGET ~= nil then
					CONFIG.BUOY.POSITION = CONFIG.BUOY.TARGET.Position
				end
			end

			v[2].MaxForce = math.huge
			v[2].MaxVelocity = math.huge
			v[3].MaxTorque = Vector3.new(math.huge,math.huge,math.huge)
			v[2].Position = CONFIG.BUOY.POSITION
			v[2].RigidityEnabled = CONFIG.BUOY.ENFORCEDRIGIDITY
			v[2].Responsiveness = CONFIG.BUOY.SPEED
			v[3].D = CONFIG.BUOY.GRYOSTUFF.SMOOTHNESS
			v[3].P = CONFIG.BUOY.GRYOSTUFF.SPEED
			if CONFIG.BUOY.FORMATION == 1 then
				v[3].CFrame = CFrame.lookAt(v[1].Position,CONFIG.BUOY.POSITION)
			elseif CONFIG.BUOY.FORMATION ==2 then
				v[3].CFrame = CFrame.new()*CFrame.fromEulerAngles(math.rad(math.random(1,360)),math.rad(math.random(1,360)),math.rad(math.random(1,360)))
			elseif CONFIG.BUOY.FORMATION == 3 then
				local Funny = Vector3.new(math.random(-(_G.MalevolentShrineRadius),(_G.MalevolentShrineRadius)),math.random(-(_G.MalevolentShrineRadius),(_G.MalevolentShrineRadius)),math.random(-(_G.MalevolentShrineRadius),(_G.MalevolentShrineRadius)))
				v[2].Position = Funny
				v[3].CFrame = CFrame.new()*CFrame.fromEulerAngles(math.rad(math.random(1,360)),math.rad(math.random(1,360)),math.rad(math.random(1,360)))
			end
		else
			v[2].RigidityEnabled =false
			v[2].MaxForce = 0
			v[2].MaxVelocity = 0
			v[3].MaxTorque = Vector3.new(0,0,0)
		end
	end	
	--BUOY STUFF END
	end)
end
