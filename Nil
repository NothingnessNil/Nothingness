	if Property ~= "Text" then return end
	CONFIG.GOKUSTUFF.FRAMETIME =tonumber(GuiNChildren["WaitTimeINP"].Text);
end)
GuiNChildren["GarageYeetBTN"].Activated:Connect(function()
	local Number =  tonumber(GuiNChildren["GarageNoINP"].Text)
	if Number == 1 then
		SERVICES.Player.LocalPlayer.Character:PivotTo(CFrame.new(-2595, 2.5, -2677))
	elseif Number == 2 then
		SERVICES.Player.LocalPlayer.Character:PivotTo(CFrame.new(-1503, 2.5, -2682))
	elseif Number == 3 then
		SERVICES.Player.LocalPlayer.Character:PivotTo(CFrame.new(-404.493, 2.75, -2698))
	elseif Number == 4 then
		SERVICES.Player.LocalPlayer.Character:PivotTo(CFrame.new(202, 2.5, -1831))
	elseif Number == 5 then
		SERVICES.Player.LocalPlayer.Character:PivotTo(CFrame.new(193, 2.5, -352))
	elseif Number == 6 then
		SERVICES.Player.LocalPlayer.Character:PivotTo(CFrame.new(-400, 2.5, 500))
	elseif Number == 7 then
		SERVICES.Player.LocalPlayer.Character:PivotTo(CFrame.new(-1493, 2.5, 510))
	elseif Number == 8 then
		SERVICES.Player.LocalPlayer.Character:PivotTo(CFrame.new(-2604, 2.5, 514))
	elseif Number == 9 then
		SERVICES.Player.LocalPlayer.Character:PivotTo(CFrame.new(-3101, 2.5, -335))
	elseif Number == 10 then
		SERVICES.Player.LocalPlayer.Character:PivotTo(CFrame.new(-3109, 2.75, -1837))
	else
		local Text = string.upper(GuiNChildren["GarageNoINP"].Text)
		if Text == "AMONG" then
			SERVICES.Player.LocalPlayer.Character:PivotTo(CFrame.new(-42214.273, 16.586, 41624.996))
		elseif Text == "MOON" then
			SERVICES.Player.LocalPlayer.Character:PivotTo(CFrame.new(-2118, 99860.5, -961))
		end
		GuiNChildren["GarageNoINP"].Text = "INVALID NUMBER"
	end
end)
