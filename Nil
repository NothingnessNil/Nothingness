	PARTS = nil,
	NUMBER = nil
}
repeat
	task.wait()
until (SERVICES.Player.LocalPlayer.Team.Name ~= nil)
OURGARRAGE.NUMBER = tonumber(string.sub(SERVICES.Player.LocalPlayer.Team.Name,8,8))
OURGARRAGE.PARTS = SERVICES.Work.Garages["Garage" .. string.sub(SERVICES.Player.LocalPlayer.Team.Name,7,8)]:FindFirstChild("Parts")
