GuiNChildren["SpeedTextBox"].Changed:Connect(function(Property)
	if Property ~= "Text" then return end
	local Num =  tonumber(GuiNChildren["SpeedTextBox"].Text);
	if Num == nil then
		Num = 1
	end
	if Num < 0 then
		Num = 0
	end
	CONFIG.BUOY.SPEED =Num;
end)

