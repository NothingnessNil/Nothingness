local IMMUNESOUNDS = {}
local DB = false
function OpenCheck()
	if DB == true then return end
	if MAIN.Visible == false then
		DB = true
		MAIN.Visible = true
		local Inf = TweenInfo.new(1,Enum.EasingStyle.Sine,Enum.EasingDirection.In,0,false,0)
		SERVICES.Tween:Create(MAIN,Inf,{Size =UDim2.new(0.707,0,0.44,0),Position = UDim2.new(MAIN.Position.X.Scale/2,0,MAIN.Position.Y.Scale/2,0) }):Play()
		task.wait(1)
		DB = false
		_G.Draggable = true
	else
		DB = true
		_G.Draggable = false
		local Inf = TweenInfo.new(1,Enum.EasingStyle.Back,Enum.EasingDirection.In,0,false,0)
		SERVICES.Tween:Create(MAIN,Inf,{Size =UDim2.new(0,0,0,0),Position = UDim2.new(MAIN.Position.X.Scale*2,0,MAIN.Position.Y.Scale*2,0) }):Play()
		task.wait(1)
		MAIN.Visible = false
		DB = false
	end
end
BUTTON.MouseButton1Down:Connect(OpenCheck)
