local GuiNChildren = {}
for i,v in pairs(MAIN:GetDescendants()) do
	if (v.Name == "TITLE") then continue end
	if (v.Name == "UICorner") then continue end
	if (v.Name == "UIStroke") then continue end
	GuiNChildren[v.Name] = v;
end
local PAGEBTN = script.PAGEBTN
local PagesInOrderFFS = {
	nil,nil,nil,nil,nil
}
for i,v in pairs(PAGES:GetChildren()) do
	if not (v:IsA("Frame")) then continue end
	local NUM = tonumber(string.sub(v.Name,1,1))
	PagesInOrderFFS[NUM] = v
end
for i,v in pairs(PagesInOrderFFS) do
	local PGClone = PAGEBTN:Clone()
	PGClone.Text =string.sub(v.Name,2,string.len(v.Name))
	PGClone.Activated:Connect(function()
		for i,t in pairs(PAGES:GetChildren()) do
			if not (t:IsA("Frame")) then continue end
			t.Visible =false
		end
		v.Visible = true
	end)
	PGClone.Parent = GuiNChildren["Page Scrolling"]

end
MAIN.Dragify.Enabled = true
